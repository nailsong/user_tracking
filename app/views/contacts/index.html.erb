<%= link_to "Home", home_path %>

<p id="notice"><%= notice %></p>

<h1>Contato</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Novo contato', new_contact_path %>

<script type="text/javascript">window.onload = REPORT.track()</script>